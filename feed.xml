---
layout: none
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
<title type="text">{{ site.title }}</title>
<subtitle type="text">{{ site.tagline }}</subtitle>
<generator uri="https://github.com/mojombo/jekyll">Jekyll</generator>
<atom:link rel="self" type="application/atom+xml" href="{{ site.url }}/feed.xml" />
<link rel="alternate" type="text/html" href="{{ site.url }}" />
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{ site.url }}/</id>
<author>
<name>{{ site.owner.name }}</name>
<uri>{{ site.url }}/</uri>
<email>{{ site.owner.email }}</email>
</author>
{% for post in site.posts limit:20 %}

<item>
<title><![CDATA[ {{ post.title }} ]]>></title>
<link>{{ site.url }}{{ post.url }}</link>
<category><![CDATA[ {{ post.categoryz }} ]]></category>
<id>{{ site.url }}{{ post.id }}</id>
{% if post.modified %}<atom:updated>{{ post.modified | to_xmlschema }}T00:00:00-00:00</atom:updated>
<published>{{ post.date | date_to_xmlschema }}</published>
{% else %}<published>{{ post.date | date_to_xmlschema }}</published>
<atom:updated>{{ post.date | date_to_xmlschema }}</atom:updated>{% endif %}
<author>
<name>{{ site.owner.name }}</name>
<uri>{{ site.url }}</uri>
<email>{{ site.owner.email }}</email>
</author>
<image>
    <url>https://gaminggeek.dev/assets/firecover.png</url>
    <title>Fire</title>
    <link>https://fire.gaminggeek.dev/</link>
  </image>
<content:encoded><![CDATA[{{ post.content | xml_escape }}]]></content:encoded>
</item>
{% endfor %}
 </feed>